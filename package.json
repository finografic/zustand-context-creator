    "@types/json-schema": "7.0.15",
    "@types/node": "22.15.17",
    "@types/prompts": "^2.4.9",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/parser": "^8.32.0",
    "@typescript-eslint/rule-tester": "8.32.0",
