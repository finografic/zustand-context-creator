{
  "name": "@finografic/zustand-context-creator",
  "type": "module",
  "version": "0.1.0",
  "description": "Fingorafic Zustix Context Creator",
  "author": {
    "name": "Justin",
    "email": "justin.blair.rankin@gmail.com",
    "url": "http://finografic.github.com/cv-justin-rankin"
  },
  "license": "MIT",
  "keywords": [
    "zustex",
    "zustand",
    "context"
  ],
  "packageManager": "pnpm@10.0.0",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "eslint-config": "bin/index.js"
  },
  "files": [
    "bin",
    "dist"
  ],
  "eslintConfig": {
    "extends": [
      "./dist"
    ]
  },
  "scripts": {
    "========== DEV": "=======================================================",
    "dev": "npx @eslint/config-inspector --config eslint.config.ts",
    "watch": "tsup --format esm,cjs --watch",
    "cli": "tsx scripts/script-executor.ts --config=publish",
    "·········· BUILD": "·····················································",
    "build": "pnpm typegen && tsup --format esm,cjs --clean --dts",
    "build.inspector": "pnpm build && npx @eslint/config-inspector build",
    "·········· DOCS": "·····················································",
    "docs": "vitepress dev docs",
    "docs.md": "pnpm docs.md.clean && pnpm docs.md.build",
    "docs.md.clean": "del './docs/**/*.md'",
    "docs.md.build": "NODE_PATH=./node_modules tsx scripts/generate-md-docs",
    "docs.md.build.V1": "NODE_PATH=./node_modules tsx scripts-V1/generate-md-docs.ts",
    "docs.md.build.V1.ORIG": "NODE_PATH=./node_modules tsx scripts/generate-md-docs-V1.ts",
    "docs.press": "pnpm docs.press.clean && pnpm docs.press.build",
    "docs.press.clean": "del './docs/.vitepress/{cache,dist}'",
    "docs.press.build": "vitepress build docs",
    "·········· PUBLISH.GITHUB": "·····················································",
    "prepublish.github": "pnpm build && git add dist/** && git commit -m \"chore: update generated files\" || true",
    "publish.github.patch": "pnpm prepublish.github && dotenvx run -- pnpm version patch && pnpm publish --registry https://npm.pkg.github.com",
    "publish.github.minor": "pnpm prepublish.github && dotenvx run -- pnpm version minor && pnpm publish --registry https://npm.pkg.github.com",
    "publish.github.major": "pnpm prepublish.github && dotenvx run -- pnpm version major && pnpm publish --registry https://npm.pkg.github.com",
    "·········· REGISTRIES": "··············································",
    "view.registry.gitub": "dotenvx run -- pnpm view @finografic/eslint-config --registry=https://npm.pkg.github.com",
    "·········· UPDATE.DEPS": "·····················································",
    "upgrade.deps.all": "pnpm update --latest",
    "·········· LINTING": "···················································",
    "lint": "eslint .",
    "lint.dry": "eslint . --dry-run",
    "lint.fix": "eslint . --fix",
    "inspect.eslint-config": "eslint-config-inspector",
    "·········· UTILS": "·····················································",
    "typegen": "tsx scripts/typegen.ts",
    "typecheck": "tsc --noEmit",
    "upgrade.all.deps": "pnpm update --latest",
    "·········· HOOKS": "·····················································",
    "prepack": "pnpm build",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "@eslint-react/eslint-plugin": "^1.38.4",
    "@prettier/plugin-xml": "^3.4.1",
    "@unocss/eslint-plugin": ">=0.50.0",
    "astro-eslint-parser": "^1.0.2",
    "eslint": "^9.10.0",
    "eslint-plugin-astro": "^1.2.0",
    "eslint-plugin-format": ">=0.1.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "prettier-plugin-astro": "^0.14.0"
  },
  "peerDependenciesMeta": {
    "@eslint-react/eslint-plugin": {
      "optional": true
    },
    "@prettier/plugin-xml": {
      "optional": true
    },
    "@unocss/eslint-plugin": {
      "optional": true
    },
    "astro-eslint-parser": {
      "optional": true
    },
    "eslint-plugin-astro": {
      "optional": true
    },
    "eslint-plugin-format": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "eslint-plugin-react-refresh": {
      "optional": true
    },
    "eslint-plugin-vuejs-accessibility": {
      "optional": true
    },
    "prettier-plugin-astro": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/utils": "^9.2.0",
    "@clack/prompts": "^0.10.1",
    "@dotenvx/dotenvx": "1.43.0",
    "@emotion/eslint-plugin": "11.12.0",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
    "@eslint/markdown": "^6.4.0",
